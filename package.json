{
  "name": "hiberbee-vscode-theme",
  "displayName": "Hiberbee Theme",
  "version": "0.5.0",
  "repository": {
    "url": "https://github.com/hiberbee/jetbrains-ide-theme",
    "type": "git",
    "directory": "src/vscode"
  },
  "publisher": "Hiberbee",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "start": "code",
    "build": "vsce package",
    "publish": "vsce publish --no-git-tag-version"
  },
  "contributes": {
    "themes": [
      {
        "label": "Hiberbee",
        "uiTheme": "vs-dark",
        "path": "./src/vscode/Hiberbee.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/vsce": "^2.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.3.1"
}
